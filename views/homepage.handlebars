    <header id="header">
        <div id="nav">
            <div id="header-left"></div>
            <div id="logo">
                <h1>CIRCLE</h1>
                <h3>COMMUNITY BULLETIN</h3>
            </div>
            <div id="header-right">
                <p>Welcome {{username}} &nbsp;&nbsp;|&nbsp;&nbsp;<a href="/login">login</a> &nbsp;&nbsp;|&nbsp;&nbsp;<a href="/signup">signup</a></p>
            </div>
        </div>

    </header>
    <main>
        {{> sortbar}}

        <div id="container">

            <div id='mainDiv'>
        
        
            {{#each card}}
                <div class="card">
                    <a href="/cards/{{dataValues.id}}"><h3>{{dataValues.event_name}}</h3>
                    <p>{{dataValues.event_description}}</p>
                    <h6 id="date">{{dataValues.event_date}}</h6>
                    <h6 id="time">{{dataValues.event_time}}</h6>
                    </a>
                </div>
            {{/each}}

            </div>
            
            
            
        </div>
    </main>

